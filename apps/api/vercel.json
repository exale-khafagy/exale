{
  "version": 2,
  "installCommand": "npm install && npm run db:generate && npm run build",
  "builds": [
    { "src": "dist/src/server.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "dist/src/server.js", "methods": ["GET", "POST", "PUT", "PATCH", "DELETE", "OPTIONS"] }
  ]
}
